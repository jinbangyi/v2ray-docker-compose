{
  "reverse": {
    "bridges": [
      {
        "tag": "bridge",
        "domain": "private.cloud.com"
      }
    ]
  },
  "outbounds": [
    {
      "tag": "tunnel",
      "protocol": "vless",
      "settings": {
        "vnext": [
          {
            "address": "<RELAY-IP>",
            "port": 11310,
            "users": [
              {
                "id": "<RELAY-UUID>",
                "alterId": 0,
                "email": "t@t.tt",
                "security": "auto",
                "encryption": "none",
                "flow": ""
              }
            ]
          }
        ]
      },
      "streamSettings": {
        "network": "ws",
        "security": "tls",
        "tlsSettings": {
          "allowInsecure": true,
          "serverName": "<RELAY-IP>"
        },
        "wsSettings": {
          "path": "/science",
          "headers": {
            "Host": "<RELAY-IP>"
          }
        }
      },
      "mux": {
        "enabled": false,
        "concurrency": -1
      }
    },
    {
      "protocol": "freedom",
      "settings": {},
      "tag": "out"
    }
  ],
  "routing": {
    "rules": [
      {
        "type": "field",
        "inboundTag": ["bridge"],
        "domain": ["full:private.cloud.com"],
        "outboundTag": "tunnel"
      },
      {
        "type": "field",
        "inboundTag": ["bridge"],
        "outboundTag": "out"
      }
    ]
  }
}
