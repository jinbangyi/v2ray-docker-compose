{
  "reverse": {
    "bridges": [
      {
        "tag": "bridge",
        "domain": "private.cloud.com"
      }
    ]
  },
  "outbounds": [
    {
      "tag": "tunnel",
      "protocol": "vmess",
      "settings": {
        "vnext": [
          {
            "address": "<RELAY-IP>",
            "port": 11310,
            "users": [
              {
                "id": "<RELAY-UUID>",
                "alterId": 0
              }
            ]
          }
        ]
      }
    },
    {
      "protocol": "freedom",
      "settings": {},
      "tag": "out"
    }
  ],
  "routing": {
    "rules": [
      {
        "type": "field",
        "inboundTag": ["bridge"],
        "domain": ["full:private.cloud.com"],
        "outboundTag": "tunnel"
      },
      {
        "type": "field",
        "inboundTag": ["bridge"],
        "outboundTag": "out"
      }
    ]
  }
}
